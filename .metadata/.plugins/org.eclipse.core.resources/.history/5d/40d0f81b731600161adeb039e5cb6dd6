def sumofnumbers(x,y):
    return x+y

def divideTwoNumbers(x,y):
    return x/y

def multiplyTwoNumbers(i,j):
    return i*j

print 'sum is ', sumofnumbers(2,4)
print 'divide- ',sum(3, 2)
print 'multiply', multiplyTwoNumbers(3, 4)
