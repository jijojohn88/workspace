class node:
    def __init__(self,d,left=None,right=None):
        self.data=d
        self.left=left
        self.right=right

def partition(ar):
    if(ar == None and len(ar) <1) :
        return;
    root = node(ar[len(ar)//2])
    left = ar[:len(ar)//2]
    right= ar[len(ar)//2+1:]
    

